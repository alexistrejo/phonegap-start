function initCharts(){
    var data= new Array();
    var data2= new Array();
    var xaxis= new Array();

    for(var i=0;i<25;i++){
	data.push(Math.random());
	data2.push(Math.random());
	xaxis.push('C'+i);
    }
    window.chart = new Highcharts.StockChart({
	chart : {
	    renderTo : 'container'
	}
	,credits:{enabled:false}
	,rangeSelector : {
	    enabled : false
	}
	,title : {
	    text : 'INPC: InflaciÃ³n general, subyacente y no subyacente'
	}
	,series : [
	    {name : 'Serie 1.',
	     data : data
	     ,tooltip: {
		 valueDecimals: 2
	     }
	    }
	    ,{name : 'Serie 2',
	      data : data2
	      ,tooltip: {
		  valueDecimals: 2
	      }
	     }
	]
	,exporting :{
	    enabled : false
	}
    },function(chart){initChart(chart);});  
}
